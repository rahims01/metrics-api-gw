# Kafka Configuration
KAFKA_BOOTSTRAP_SERVERS=server1:9092,server2:9092
KAFKA_TOPIC=metrics
KAFKA_CLIENT_ID=metrics-collector

# Kerberos/GSSAPI Configuration
KRB5_CONFIG=/etc/krb5.conf
KRB5_KTNAME=/etc/kafka.keytab
KRB5_CLIENT_KTNAME=/etc/kafka.keytab
KAFKA_SASL_KERBEROS_DOMAIN_NAME=your-domain

# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=True
SESSION_SECRET=your-secret-key-here

# Application Configuration
LOG_LEVEL=DEBUG
APP_ID=your-app-id-here
CORS_ORIGINS=*